{%- liquid
  assign section_spacing_top = section.settings.section_spacing_top
  assign section_spacing_bottom = section.settings.section_spacing_bottom
  assign color_scheme = section.settings.color_scheme
  assign container_size = section.settings.container_size
  assign block_layout = section.settings.layout

  assign sectionIdWithFeaturedProductPrefix = section.id | append: '---FeaturedProduct'
-%}

{% capture css %}
  {% style %}
    .section-{{section.id}} {
        --gsc-section-spacing-top: {{ section_spacing_top }}px;
        --gsc-section-spacing-bottom: {{ section_spacing_bottom }}px;
    }
  {% endstyle %}
{% endcapture %}

{% render 'inline-css-minifier', css: css %}

<div class='product section section-{{ section.id }} color-{{ color_scheme }} section-spacing {% if block_layout == 'two-column' or block_layout == 'one-column' %} product--with-desktop-media-grid {% endif %} {% if block_layout == 'thumbnails-left' or block_layout == 'thumbnails-right' or block_layout == 'thumbnails-bottom' %}  {% if container_size == 'page-width' %} product--with-page-width-container-and-desktop-media-carousel {% else %} product--with-desktop-media-carousel {% endif %}  {% endif %}'>
  <div class='container container--{{ container_size }} product__container'>
    {% render 'product',
      product: section.settings.product,
      section: section,
      sectionId: sectionIdWithFeaturedProductPrefix
    %}
  </div>
</div>

