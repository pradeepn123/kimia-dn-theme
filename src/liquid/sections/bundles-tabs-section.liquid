<script>
  document.addEventListener('DOMContentLoaded', () => {
    const elements = document.querySelectorAll('.background_image_bundlesProduct');
    const elementsValues = Object.values(elements);

    elementsValues.forEach(item => {
      item.addEventListener('click', (e) => {
        e.preventDefault();
      })
    })

    let selectedTab = document.querySelectorAll('.background_image_bundlesProduct');
    let selectedTabValues = Object.values(selectedTab);

    selectedTabValues.map(item => {
      item.addEventListener('click', (e) => {
        let currentSelectedTabValueParent = document.querySelector('#bunldesPage_getData_id').getAttribute('data-selected-tab-id');

        const imagesTabs = document.querySelectorAll('.new_featured_image .product-details__tab-name');
        Object.values(imagesTabs).map(item => {
          if (currentSelectedTabValueParent == Number(item.getAttribute('data-id'))){
            item.parentNode.style.display = 'block';
          }
          else{
            item.parentNode.style.display = 'none';
          }
          // console.log(item.parentNode);
        });
      })
      // let currentSelectedTab = Number(item.getAttribute('data-id'));
    })
  })
</script>

{%- liquid
  assign options = section.settings

  assign style = options.style
  assign color_scheme = options.color_scheme
  assign section_spacing_top = options.section_spacing_top
  assign section_spacing_bottom = options.section_spacing_bottom
  assign container_size = options.container_size
  assign enable_background_around_container = options.enable_background_around_container
  assign expand_first_collapsible_row = options.expand_first_collapsible_row

  assign first_block = section.blocks | first

  assign bundleProducts = product.metafields.custom.bundle_contained_products.value
  assign loopingFixedValues = product.metafields.custom.powerful_benefits_extra_info.value

-%}

{% capture css %}
  {% style %}
    .section-{{ section.id }} {
      --gsc-section-spacing-top: {{ section_spacing_top }}px;
      --gsc-section-spacing-bottom: {{ section_spacing_bottom }}px;
    }

    {% for block in section.blocks %}
        .block-{{block.id}}{
            --gsc-image-width: {{ block.settings.image_width }}%;
        }
    {% endfor %}
    .heading_subHeading{
      background-color: {{ product.metafields.custom.color_schema.value.highlighted_background_color }};
    }
    .heading_subHeading .product-details__tab-name.selected{
      color: {{ product.metafields.custom.color_schema.value.highlighted_text_color }};
    }
    .bundle-product-details .product-details__container .desktop_only{
      background-color: {{ section.settings.section_bg_color }}
    }
    .bundle-product-details .product-details__container{
      background-color: {{ section.settings.section_bg_color }};
    }
  {% endstyle %}
{% endcapture %}

{% render 'inline-css-minifier', css: css %}

{% if bundleProducts.count > 0 %}
  <div class='bundle-product-details product-details section-{{ section.id }} {% if enable_background_around_container %} section color-{{ color_scheme }} {% endif %}'>
    <div class='section-spacing container--{{ container_size }}'>
      <div class='product-details__container {% unless enable_background_around_container %} product-details__container--with-aside-spacing section {% endunless %}'>
          <div class="desktop_only desktop_tabs">
              <tabs-component data-selected-tab-id='1' id="bunldesPage_getData_id">
                    <div class='product-details__tab-names heading_subHeading' data-aos="fade-up">
                        {% for itemValue in bundleProducts %}
                            {% assign loopOneValue = itemValue.metafields.custom.powerful_benefits_extra_info.value %}
                            {% if loopOneValue.subheading != blank %}
                                <div data-tabs-name class='tab-name product-details__tab-name {% if forloop.index == 1 %}selected{% endif %}' data-id='{{ forloop.index }}' tabindex='0'>
                                    <p class="main_heading">{{ section.settings.heading }}</p>
                                    <p class="main_subHeading">{{ loopOneValue.subheading }}</p>
                                </div>
                            {% endif %}
                        {%- endfor -%}
                    </div>
                  {% comment %} Tabs Title  {% endcomment %}


                  <div class='product-details__tab-names tabs_only' data-aos="fade-up">
                      {% for itemValue in bundleProducts %}
                          {% assign loopOneValue = itemValue.metafields.custom.powerful_benefits_extra_info.value %}
                          {% if loopOneValue.featured_image != blank %}
                              <div data-tabs-name class='background_image_bundlesProduct tab-name product-details__tab-name {% if forloop.index == 1 %}selected{% endif %}' data-id='{{ forloop.index }}' tabindex='0' style="background-image: url({{ loopOneValue.featured_image | img_url: 'master' }})">
                                  {{ forloop.index }}
                              </div>
                          {% endif %}
                      {%- endfor -%}
                  </div>


                    {% comment %} This is Mobile Image Starts {% endcomment %}
                      <div class="product-details__tab-names images_only">
                        {% for itemValue in bundleProducts %}
                          <div class='product-details__tab-names new_featured_image' style="{% if forloop.index == 1 %}display: block;{% else %}display: none;{% endif %}">
                                  {% assign loopOneValue = itemValue.metafields.custom.powerful_benefits_extra_info.value %}
                                  {% if loopOneValue.featured_image != blank %}

                                      <div data-tabs-name class='tab-name product-details__tab-name {% if forloop.index == 1 %}selected{% endif %}' data-id='{{ forloop.index }}' tabindex='0'>
                                          <img src="{{ loopOneValue.featured_image | img_url : 'master'}}" alt="" class="testing_img">
                                      </div>

                                  {% endif %}
                          </div>
                        {% endfor %}
                      </div>
                    {% comment %} This is Mobile Image Ends {% endcomment %}
                  {% comment %} Tabs Title Ends {% endcomment %}

                  <div class='tabs product-details__tabs' data-tabs-container>
                      <template data-tabs-elements-template>
                          {% for item in bundleProducts  offset: 1 %}

                                  <tab-component
                                  class='tab product-details__tab block-{{block.id}}'
                                  aria-hidden='true'
                                  data-id='{{ forloop.index | plus: 1 }}' >

                                      <div class='product-details__content-tab-content body-font-weight-from-global-settings'
                                          data-tabs-element-content
                                          {{ block.shopify_attributes }} >
                                              <div class='rte product-details__rte'>
                                                {% assign productBeneits = item.metafields.custom.product_benefits.value %}
                                                {% assign prodBenefitsExtraData = item.metafields.custom.powerful_benefits_extra_info.value %}
                                                  <div class="pb_container">
                                                    <div class="pb_content">
                                                        <div class="pb_left pb_styling">
                                                          {% for block in productBeneits limit: 2 %}
                                                              {% unless block.heading == "false" %}
                                                                  <div class="pb_content_continer" style="{% if block.description == blank %}min-height: fit-content;{% endif %}"> 
                                                                        {% if block.images != blank %}
                                                                          <img src="{{ block.images | img_url: 'master'}}" alt="">
                                                                        {% endif %}
                                                                      <span class="pb_content_left pb_content_inner">
                                                                          <h6>{{ block.heading }}</h6>
                                                                          <p>{{ block.description }}</p>
                                                                      </span>
                                                                  </div>
                                                              {% endunless%}
                                                              {% if block.show_horizontal_line == true %}
                                                                <hr>
                                                              {% endif %}
                                                          {% endfor %}
                                                        </div>
                                                        <div class="pb_center">
                                                            <img src="{{ prodBenefitsExtraData.featured_image | img_url: 'master'}}" alt="">
                                                        </div>
                                                        <div class="pb_right pb_styling">
                                                            {% for block in productBeneits offset: 2 %}
                                                                {% unless block.heading == "false" %}
                                                                    <div class="pb_content_continer" style="{% if block.description == blank %}min-height: fit-content;{% endif %}">
                                                                          {% if block.images != blank %}
                                                                            <img src="{{ block.images | img_url: 'master'}}" alt="">
                                                                          {% endif %}
                                                                        <span class="pb_content_right pb_content_inner">
                                                                            <h6>{{ block.heading }}</h6>
                                                                            <p>{{ block.description }}</p>
                                                                        </span>
                                                                    </div>
                                                                {% endunless %}
                                                                {% if block.show_horizontal_line == true %}
                                                                  <hr>
                                                                {% endif %}
                                                            {% endfor %}
                                                        </div>
                                                    </div>
                                                  </div>

                                                  <div class="pb_content_mobile_accordion container--page-width section"> 
                                                      {% for blockX in productBeneits %}
                                                          {% unless blockX.heading == "false" %}
                                                              <accordeon-component
                                                                  class='accordeon-plus {% unless forloop.last %} accordeon-plus--bottom-bordered{% endunless %} {% unless forloop.first %} accordeon-plus--top-bordered{% endunless %} block-{{block.id}}'
                                                                  data-aria-expanded='{% if expand_first_collapsible_row and forloop.index == 1 %}true{% else %}false{% endif %}'
                                                                  data-section-id='{{ section.id }}'
                                                                  data-block-id='{{ block.id }}'
                                                                  {{ block.shopify_attributes }} >
                                                                  <accordeon-btn
                                                                      style="{% if blockX.description == blank %}pointer-events: none{% endif %}"
                                                                      class='accordeon-plus__btn accordeon-plus__btn--small faq-section__accordeon-btn'
                                                                      tabindex='0'
                                                                  >
                                                                    {% if blockX.images != blank %}
                                                                      <img src="{{ blockX.images | img_url: 'master'}}" alt="">
                                                                    {% endif %}
                                                                      <h6>{{ blockX.heading }}</h6>
                                                                      {% if blockX.description != blank %}
                                                                        {% render 'icon', icon_name: 'plus', class: 'accordeon-plus__icon' %}
                                                                      {% endif %}
                                                                  </accordeon-btn>
                                                                  {% if blockX.description != blank %}
                                                                    <div class='accordeon-plus__content-wrap' data-accordeon-content-wrapper>
                                                                        <div class='accordeon-plus__content faq-section__accordeon-content' data-accordeon-content>

                                                                        <div class='faq-section__content-tab-content body-font-weight-from-global-settings'>
                                                                            <div class='rte faq-section__rte bundles-tabs__rte' style="{% if blockX.images == blank %}padding-left: 2rem{% endif %}">
                                                                                {{ blockX.description }}
                                                                            </div>
                                                                        </div>
                                                                        </div>
                                                                    </div>
                                                                  {% endif %}
                                                              </accordeon-component>
                                                          {% endunless %}
                                                      {% endfor %}
                                                  </div>
                                              </div>

                                      </div>
                                  </tab-component>
                          {%- endfor -%}
                      </template>

                      {%- case first_block.type -%}
                          {% assign custom_icon_image = first_block.settings.custom_icon_image %}
                          {% assign icon = first_block.settings.icon %}

                      {%- when 'collapsible_row' -%}
                        {% for item in bundleProducts limit: 1 %}
                          <tab-component
                          class='tab product-details__tab block-{{first_block.id}}'
                          aria-hidden='false'
                          data-id='1' >
                          {%- assign text = first_block.settings.text -%}

                          <div class='product-details__content-tab-content body-font-weight-from-global-settings'
                              data-tabs-element-content
                              {{ first_block.shopify_attributes }} >
                                      <div class='rte product-details__rte'>
                                            {% assign productBeneits = item.metafields.custom.product_benefits.value %}
                                                {% assign prodBenefitsExtraData = item.metafields.custom.powerful_benefits_extra_info.value %}
                                                  
                                                  <div class="pb_container" >
                                                    <div class="pb_content">
                                                        <div class="pb_left pb_styling">
                                                          {% for block in productBeneits limit: 2 %}
                                                              {% unless block.heading == "false" %}
                                                                  <div class="pb_content_continer">
                                                                      {% if block.images != blank %}
                                                                        <img src="{{ block.images | img_url: 'master'}}" alt="">
                                                                      {% endif %}
                                                                      <span class="pb_content_left pb_content_inner">
                                                                          <h6>{{ block.heading }}</h6>
                                                                          <p>{{ block.description }}</p>
                                                                      </span>
                                                                  </div>
                                                              {% endunless%}
                                                                {% if block.show_horizontal_line == true %}
                                                                  <hr>
                                                                {% endif %}
                                                          {% endfor %}
                                                        </div>
                                                        <div class="pb_center" data-aos="fade-up" data-aos-delay="400">
                                                            <img src="{{ prodBenefitsExtraData.featured_image | img_url: 'master'}}" alt="">
                                                        </div>
                                                        <div class="pb_right pb_styling">
                                                            {% for block in productBeneits offset: 2 %}
                                                                {% unless block.heading == "false" %}
                                                                    <div class="pb_content_continer">
                                                                        {% if block.images != blank %}
                                                                          <img src="{{ block.images | img_url: 'master'}}" alt="">
                                                                        {% endif %}
                                                                        <span class="pb_content_right pb_content_inner">
                                                                            <h6>{{ block.heading }}</h6>
                                                                            <p>{{ block.description }}</p>
                                                                        </span>
                                                                    </div>
                                                                {% endunless %}
                                                                  {% if block.show_horizontal_line == true %}
                                                                    <hr>
                                                                  {% endif %}
                                                            {% endfor %}
                                                        </div>
                                                    </div>
                                                  </div>

                                                  <div class="pb_content_mobile_accordion container--page-width section">
                                                      {% for blockX in productBeneits %}
                                                          {% unless blockX.heading == "false" %}
                                                              <accordeon-component
                                                                  class='accordeon-plus {% unless forloop.last %} accordeon-plus--bottom-bordered{% endunless %} {% unless forloop.first %} accordeon-plus--top-bordered{% endunless %} block-{{block.id}}'
                                                                  data-aria-expanded='{% if expand_first_collapsible_row and forloop.index == 1 %}true{% else %}false{% endif %}'
                                                                  data-section-id='{{ section.id }}'
                                                                  data-block-id='{{ block.id }}'
                                                                  {{ block.shopify_attributes }} >
                                                                  <accordeon-btn
                                                                      class='accordeon-plus__btn accordeon-plus__btn--small faq-section__accordeon-btn'
                                                                      tabindex='0'
                                                                  >
                                                                    {% if blockX.images != blank %}
                                                                      <img src="{{ blockX.images | img_url: 'master'}}" alt="">
                                                                    {% endif %}
                                                                      <h6>{{ blockX.heading }}</h6>
                                                                      {% render 'icon', icon_name: 'plus', class: 'accordeon-plus__icon' %}
                                                                  </accordeon-btn>
                                                                  <div class='accordeon-plus__content-wrap' data-accordeon-content-wrapper>
                                                                      <div class='accordeon-plus__content faq-section__accordeon-content' data-accordeon-content>

                                                                      <div class='faq-section__content-tab-content body-font-weight-from-global-settings'>
                                                                          <div class='rte faq-section__rte bundles-tabs__rte' style="{% if blockX.images == blank %}padding-left: 2rem{% endif %}">
                                                                              {{ blockX.description }}
                                                                          </div>
                                                                      </div>
                                                                      </div>
                                                                  </div>
                                                              </accordeon-component>
                                                          {% endunless %}
                                                      {% endfor %}
                                                  </div>
                                      </div>
                          </div>
                          </tab-component>
                        {% endfor %}
                      {%- endcase -%}
                  </div>
              </tabs-component>
          </div>
      </div>
    </div>
  </div>
{% endif %}

{% schema %}
{
  "name": "Bundles Tabs Section",
  "tag": "section",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading"
    },
    {
      "type": "color",
      "id": "heading_color",
      "label": "Heading Color"
    },
    {
      "type": "color",
      "id": "heading_bg_color",
      "label": "Heading Background Color"
    },
    {
      "type": "color",
      "id": "section_bg_color",
      "label": "Section Background Color"
    },
    {
      "type": "select",
      "id": "color_scheme",
      "options": [
        {
          "value": "default",
          "label": "t:sections.general_section_settings.color_scheme_option_default_label"
        },
        {
          "value": "primary",
          "label": "t:sections.general_section_settings.color_scheme_option_primary_label"
        },
        {
          "value": "secondary",
          "label": "t:sections.general_section_settings.color_scheme_option_secondary_label"
        },
        {
          "value": "custom-1",
          "label": "t:sections.general_section_settings.color_scheme_option_custom_1_label"
        },
        {
          "value": "custom-2",
          "label": "t:sections.general_section_settings.color_scheme_option_custom_2_label"
        },
        {
          "value": "custom-3",
          "label": "t:sections.general_section_settings.color_scheme_option_custom_3_label"
        }
      ],
      "default": "custom-1",
      "label": "t:sections.general_section_settings.color_scheme_label",
      "info": "t:sections.general_section_settings.color_scheme_tip"
    },
    {
      "type": "checkbox",
      "id": "enable_background_around_container",
      "default": true,
      "label": "t:sections.general_section_settings.enable_background_around_container_label"
    },
    {
      "type": "select",
      "id": "container_size",
      "options": [
        {
          "value": "page-width",
          "label": "t:sections.general_section_settings.container_size.page_width_option"
        },
        {
          "value": "large",
          "label": "t:sections.general_section_settings.container_size.large_width_option"
        },
        {
          "value": "medium",
          "label": "t:sections.general_section_settings.container_size.medium_width_option"
        },
        {
          "value": "small",
          "label": "t:sections.general_section_settings.container_size.small_width_option"
        },
        {
          "value": "xsmall",
          "label": "t:sections.general_section_settings.container_size.extra_small_width_option"
        }
      ],
      "default": "large",
      "label": "t:sections.general_section_settings.container_size.label"
    },
    {
      "type": "range",
      "id": "section_spacing_top",
      "min": 0,
      "max": 100,
      "step": 1,
      "default": 0,
      "unit": "t:sections.general_section_settings.section_spacing_top.unit",
      "label": "t:sections.general_section_settings.section_spacing_top.label"
    },
    {
      "type": "range",
      "id": "section_spacing_bottom",
      "min": 0,
      "max": 100,
      "step": 1,
      "default": 0,
      "unit": "t:sections.general_section_settings.section_spacing_bottom.unit",
      "label": "t:sections.general_section_settings.section_spacing_bottom.label"
    },
    {
      "type": "checkbox",
      "default": false,
      "id": "expand_first_collapsible_row",
      "label": "t:sections.product_details.expand_first_collapsible_row.label",
      "info": "t:sections.product_details.expand_first_collapsible_row.info"
    }
  ],
  "blocks": [
    {
      "type": "collapsible_row",
      "name": "t:sections.product_details.collapsible_row_block_name",
      "settings": [
        {
          "type": "text",
