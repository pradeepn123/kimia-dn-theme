{%- liquid
  assign options = section.settings

  assign color_scheme = options.color_scheme
  assign section_spacing_top = options.section_spacing_top
  assign section_spacing_bottom = options.section_spacing_bottom
  assign title = options.title
  assign text = options.text
  assign container_size = options.container_size
  assign body_size = options.body_size
  assign heading_size = options.heading_size
  assign btn_url = options.btn_url
  assign btn_style = options.btn_style
  assign btn_text = options.btn_text
  assign section_layout = options.section_layout
-%}

<link rel="stylesheet" href="{{ 'bundle.faq.css' | asset_url }}">

{% if page.handle == 'rewards' %}
  {{ 'rewards.css' | asset_url | stylesheet_tag }}
{% endif %}

{% capture css %}
  {% style %}
    .section-{{ section.id }} {
      --gsc-section-spacing-top: {{ section_spacing_top }}px;
      --gsc-section-spacing-bottom: {{ section_spacing_bottom }}px;
    }

    {% for block in section.blocks %}
      .block-{{ block.id }} {
        --gsc-image-width: {{ block.settings.image_width }}%;
      }
    {% endfor %}
  {% endstyle %}
{% endcapture %}

{% render 'inline-css-minifier', css: css %}

<div class='faq section section-{{ section.id }} color-{{ color_scheme }} faq--{{ section_layout }}'>
  <div class='faq--main--parent faq__container container container--{{ container_size }} section-spacing {% if section_layout == 'left-tabs' %} faq__container--with-tabs-on-left-side {% elsif section_layout == 'right-tabs' %} faq__container--with-tabs-on-right-side {% elsif section_layout == 'bottom-tabs' %} faq__container--with-tabs-on-bottom-side {% endif %}'>
    {%- if title != blank or text != blank or btn_text != blank -%}
      <div class='faq__content {% if container_size == 'full-screen' and settings.edge_rounding_degree != "none" %}{% if section_layout == 'left-tabs' %} faq__content--with-right-spacing {% elsif section_layout == 'right-tabs' %} faq__content--with-left-spacing {% endif %}{% endif %}'>
        {%- if title != blank -%}
          <h5 class='faq__content-heading' style ="color: {{ section.settings.head_color }}">{{ title }}</h5>
        {%- endif -%}
        {%- if text != blank -%}
          <div class='rte faq__content-text'>{{ text }}</div>
        {%- endif -%}
        {%- if btn_text != blank -%}
          <a
            class='btn btn--{{ btn_style }} faq__content-btn'
            href='{% if btn_url != blank %}{{ btn_url }}{% else %}#{% endif %}'
            aria-label='{{- btn_text -}}'
          >
            {{- btn_text -}}
          </a>
        {%- endif -%}
      </div>
    {%- endif -%}
    {%- if section.blocks.size > 0 -%}
      <div class='faq__blocks'>
        {%- for block in section.blocks -%}
          {%- assign icon = block.settings.icon -%}
          {%- assign custom_icon_image = block.settings.custom_icon_image -%}

          {%- case block.type -%}
            {%- when 'category' -%}
              <div class='faq__category block-{{ block.id }}' {{ block.shopify_attributes }} style = "background-color:{{ block.settings.faq_heading_bg }}; font-size:{{ block.settings.head_font_size }}px; color : {{ block.settings.heading_color }};">
                {%- if custom_icon_image != blank -%}
                  <img
                    class='faq__category-icon lazyload'
                    {% render 'image-attributes', image: custom_icon_image, size: 'small' %}
                  >
                {%- elsif icon != 'none' -%}
                  {% render 'icon-pack', icon: icon, class: 'faq__category-icon' %}
                {%- endif -%}
                {{ block.settings.title }}
              </div>
            {%- when 'question' -%}
              {%- assign image = block.settings.image -%}
              {%- assign video = block.settings.video -%}
              {%- assign page = block.settings.page -%}

              <accordeon-component
                class='faq__question faq__accordeon-plus accordeon-plus accordeon-plus--bottom-bordered accordeon-plus--top-bordered block-{{ block.id }}'
                data-aria-expanded='false'
                data-section-id='{{ section.id }}'
                data-block-id='{{ block.id }}'
                {{ block.shopify_attributes }}
              >
                <accordeon-btn class='faq__accordeon-plus-btn accordeon-plus__btn' style="font-size: {{ block.settings.subhead_fontsize }}px;" tabindex='0'>
                  {%- if custom_icon_image != blank -%}
                    <img
                      class='accordeon-plus__btn-icon lazyload'
                      {% render 'image-attributes', image: custom_icon_image, size: 'small' %}
                    >
                  {%- elsif icon != 'none' -%}
                    {% render 'icon-pack', icon: icon, class: 'accordeon-plus__btn-icon' %}
                  {%- endif -%}

                  {{ block.settings.title }}

                  {% render 'icon', icon_name: 'plus', class: 'accordeon-plus__icon' %}
                </accordeon-btn>
                <div class='accordeon-plus__content-wrap' data-accordeon-content-wrapper>
                  <div
                    class='faq__accordeon-plus-content accordeon-plus__content body-font-weight-from-global-settings' style="font-size: {{ block.settings.content_font }}px;"
                    data-accordeon-content
                  >
                    {% if block.settings.text != blank %}
                      <div class='rte'>
                        {{ block.settings.text }}
                      </div>
                    {% endif %}
                    {% if page != blank %}
                      <div class='rte rte--page'>
                        {{ page.content }}
                      </div>
                    {% endif %}
                    {%- if video != blank -%}
                      {% render 'video-with-alt', video: video, class: 'faq__question-image' %}
                    {%- elsif image != blank -%}
                      <img
                        class='faq__question-image lazyload'
                        {% render 'image-attributes', image: image %}
                      >
                    {%- endif -%}
                  </div>
                </div>
              </accordeon-component>
          {%- endcase -%}
        {%- endfor -%}
      </div>
    {%- endif -%}
  </div>
</div>

